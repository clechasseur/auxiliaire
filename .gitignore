.idea/
.vscode/

# docker-compose.override.yml is meant to be local-only
docker-compose.override.yml

# Profiling files generated by cargo-tarpaulin and cargo-llvm-cov
*.profraw
cobertura.xml
tarpaulin-report.*
cov.json
codecov.json

# Local environment overrides
.env

# Always include .keep files
!**/.keep
